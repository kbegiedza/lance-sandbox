{
	"name": "Jupyter Base",
	"build": {
		"dockerfile": "Dockerfile"
	},
	"features": {
		"ghcr.io/devcontainers/features/docker-in-docker:2.5.0": {
			"moby": false
		}
	},
	"remoteUser": "jovyan",
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-toolsai.jupyter-keymap",
				"ms-toolsai.jupyter",
				"ms-python.python",
				"ms-python.vscode-pylance"
			]
		}
	}
}